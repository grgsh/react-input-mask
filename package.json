    "chai": "^6.2.0",
    "console-polyfill": "^0.3.0",
    "core-js": "^3.38.1",
    "cross-env": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "html-webpack-plugin": "^5.6.4",
    "husky": "^9.1.7",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-mocha": "^2.0.1",
    "karma-webpack": "^5.0.1",
    "lint-staged": "^16.2.3",
    "mocha": "^11.7.4",
    "prettier": "^3.6.2",
    "puppeteer": "^24.23.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "regenerator-runtime": "^0.14.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.52.4",
    "typescript": "^5.9.3",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "main": "index.js",
  "module": "lib/react-input-mask.esm.js",
  "exports": {
    ".": {
      "import": "./lib/react-input-mask.esm.js",
      "require": "./index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "source": "./src/index.tsx",
  "files": [
    "lib",
    "dist"
  ],
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint"
  },
  "scripts": {
    "clean": "rimraf lib dist",
    "build": "cross-env BABEL_ENV=rollup rollup -c",
    "watch": "cross-env BABEL_ENV=rollup rollup -c --watch",
    "watch:dev": "cross-env BABEL_ENV=rollup rollup -c rollup.dev.config.mjs --watch",
    "start": "cross-env BABEL_ENV=test NODE_ENV=development webpack-dev-server",
    "prepare": "husky && npm run lint && npm test && npm run clean && npm run types && npm run build",
    "lint": "eslint ./src/**/*.{js,ts,tsx} ./tests/**/*.{js,ts,tsx} ./*.{js,ts,tsx}",
    "lint-fix": "eslint --fix ./src/**/*.{js,ts,tsx} ./tests/**/*.{js,ts,tsx} ./*.{js,ts,tsx}",
    "test": "npm run build && npm run test:input && npm run test:server-render && npm run test:build",
    "test:input": "cross-env NODE_ENV=test BABEL_ENV=test karma start",
    "test:server-render": "cross-env BABEL_ENV=test mocha --require @babel/register ./tests/server-render",
    "test:build": "cross-env BABEL_ENV=test mocha --require @babel/register ./tests/build",
    "types": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mona-health/react-input-mask.git"
  },
  "dependencies": {
    "invariant": "^2.2.4",
    "prop-types": "^15.8.1",
    "warning": "^4.0.3"
  }
}
