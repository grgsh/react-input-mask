{
  "name": "@grgsh/react-input-mask",
  "description": "Masked input component for React (improved fork of react-input-mask)",
  "version": "4.0.0",
  "homepage": "https://github.com/grgsh/react-input-mask",
  "license": "MIT",
  "author": "Nikita Lobachev <sanniassin@gmail.com>",
  "contributors": [
    "Ian Channing <ian_channing@hotmail.com>",
    "Grzegorz Ilnicki <dev@grg.sh>"
  ],
  "keywords": [
    "react",
    "input",
    "mask",
    "masked",
    "react-component",
    "react-input-mask"
  ],
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.4",
    "@babel/eslint-parser": "^7.28.4",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/plugin-transform-proto-to-assign": "^7.27.1",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/register": "^7.28.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.2",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/invariant": "^2.2.37",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@types/warning": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
